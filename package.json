{
  "name": "openguideapp",
  "version": "0.0.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "compile": "tsc --noEmit -p . --pretty",
    "format": "prettier --write \"app/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint App.tsx app test --fix --ext .js,.ts,.tsx && npm run format",
    "patch": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:maestro": "maestro test .maestro/FavoritePodcast.yaml",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "patch-package",
    "bundle:visualize": "npx react-native-bundle-visualizer",
    "bundle:visualize:dev": "npx react-native-bundle-visualizer --dev",
    "build:ios:sim": "eas build --profile development --platform ios",
    "build:ios:dev": "eas build --profile development:device --platform ios",
    "build:ios:pre": "eas build --profile preview:device --platform ios",
    "build:ios:prod": "eas build --profile production --platform ios",
    "build:android:sim": "eas build --profile development --platform android --local",
    "build:android:dev": "eas build --profile development:device --platform android --local",
    "build:android:prod": "eas build --profile production --platform android --local",
    "start": "bunx expo start",
    "dev": "bunx expo start --dev-client",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "bundle:web": "npx expo export --platform web",
    "serve:web": "npx server dist",
    "prebuild:clean": "npx expo prebuild --clean",
    "prebuild": "npx expo install --check && npx expo prebuild"
  },
  "dependencies": {
    "@expo-google-fonts/space-grotesk": "^0.2.3",
    "@expo-google-fonts/space-mono": "^0.2.3",
    "@expo/config": "^9.0.2",
    "@expo/metro-runtime": "^3.2.1",
    "@iarna/toml": "^2.2.5",
    "@octokit/rest": "^21.0.0",
    "@react-native-community/slider": "4.5.2",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@shopify/flash-list": "^1.6.4",
    "apisauce": "3.0.1",
    "axios": "^1.7.2",
    "blurhash-from-url": "^0.0.8",
    "change-case": "^5.4.4",
    "css-convert-json": "^1.1.0",
    "date-fns": "^3.6.0",
    "deepmerge": "^4.3.1",
    "eslint-plugin-import": "^2.29.1",
    "expo": "~51.0.14",
    "expo-application": "~5.9.1",
    "expo-av": "~14.0.5",
    "expo-build-properties": "~0.12.3",
    "expo-dev-client": "~4.0.18",
    "expo-font": "~12.0.7",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "fast-xml-parser": "^4.4.0",
    "i18n-js": "4.4.3",
    "iconoir-react-native": "^7.7.0",
    "mdast-normalize-react-native": "^3.2.0",
    "mdast-util-from-markdown": "^2.0.1",
    "mobx": "6.12.4",
    "mobx-react-lite": "4.0.7",
    "mobx-state-tree": "6.0.0",
    "npm": "^10.8.1",
    "react": "18.3.1",
    "react-native": "0.74.2",
    "react-native-basic-carousel": "^1.1.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-drawer-layout": "4.0.0-alpha.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.17.1",
    "react-native-logs": "^5.1.0",
    "react-native-maps": "^1.15.6",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.12.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.32.0",
    "react-native-svg": "15.3.0",
    "rehype-stringify": "^10.0.0",
    "remark-directive": "^3.0.0",
    "remark-extract-frontmatter": "^3.2.0",
    "remark-frontmatter": "^5.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "toml": "^3.0.0",
    "unified": "^11.0.5",
    "unist-util-flatmap": "^1.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "vfile": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-class-static-block": "^7.21.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.24.7",
    "@babel/plugin-transform-shorthand-properties": "^7.24.7",
    "@babel/plugin-transform-template-literals": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/runtime": "^7.24.7",
    "@types/i18n-js": "3.8.9",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "babel-jest": "^29.7.0",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.2.0",
    "eslint-plugin-react": "7.34.3",
    "eslint-plugin-react-native": "4.1.0",
    "eslint-plugin-reactotron": "^0.1.4",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.2",
    "patch-package": "8.0.0",
    "postinstall-prepare": "2.0.0",
    "prettier": "3.3.2",
    "react-native-svg-transformer": "^1.4.0",
    "react-test-renderer": "18.3.1",
    "reactotron-core-client": "^2.9.3",
    "reactotron-mst": "^3.1.9",
    "reactotron-react-js": "^3.3.14",
    "reactotron-react-native": "^5.1.7",
    "reactotron-react-native-mmkv": "^0.2.6",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "5.5.2"
  },
  "expo": {
    "install": {
      "exclude": [
        "typescript"
      ]
    }
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "simple-import-sort",
      "react",
      "react-native",
      "reactotron"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/indent": 0,
      "@typescript-eslint/member-delimiter-style": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error",
      "comma-dangle": 0,
      "multiline-ternary": 0,
      "no-undef": 0,
      "no-unused-vars": 0,
      "no-use-before-define": 0,
      "no-global-assign": 0,
      "quotes": 0,
      "react-native/no-raw-text": 0,
      "react/no-unescaped-entities": 0,
      "react/prop-types": 0,
      "space-before-function-paren": 0,
      "reactotron/no-tron-in-production": "error"
    },
    "overrides": [
      {
        "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
        "rules": {
          "simple-import-sort/imports": [
            "error",
            {
              "groups": [
                ["^react", "^@?\\w"],
                ["^(@|components)(/.*|$)"],
                ["^\\u0000"],
                ["^\\.\\.(?!/?$)", "^\\.\\./?$"],
                ["^\\./(?=.*/)(?!/?$)", "^\\.(?!/?$)", "^\\./?$"],
                ["^.+\\.?(css)$"]
              ]
            }
          ]
        }
      }
    ]
  }
}